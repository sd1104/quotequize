<div class="row">
  <% @books.each do |book| %>
    <div class="col.s4.m6">
      <div class="card">
        <%= link_to book_path(book) do %>
          <span class="card-title">
            <%= book.title %>
          </span>
        <% end %>
      </div>
    </div>
  <% end %>
</div>
